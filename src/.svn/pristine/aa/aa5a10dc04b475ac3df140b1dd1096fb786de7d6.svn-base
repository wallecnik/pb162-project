package cz.muni.fi.pb162.project.geometry;

/**
 * Solid 2D geometries with seamless surface. Solid 2D geometry is always 
 * measurable and has meaningfull area.
 * 
 * @author Radek Oslejsek, oslejsek@fi.muni.cz
 */
public interface Solid extends Measurable {
    
    /**
     * Return area of a solid object.
     * 
     * @return Area
     */
    double getArea();
}
